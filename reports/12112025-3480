        <script>
            // Variables globales
            let allData = [];
            let filteredData = {
                subjects: [],
                messages: [],
                calls: [],
                attachments: [],
                contacts: [],
            };
            let messageGroupingEnabled = true;
            let messageSortOrder = "desc";
            // Estado de selecci√≥n por canal: channelKey -> boolean
            let channelSelectionMode = {};
            // Mensajes seleccionados por canal: channelKey -> Set(messageId)
            let channelSelectedMessages = {};

            FormatPhone = function (phone) {
                // Formatear n√∫mero de tel√©fono como (XXX) XXX-XXXX

                phone = phone.replace(/\D/g, ""); // Eliminar caracteres no num√©ricos
                if (phone.length !== 10) return phone; // Retornar sin cambios si no es de 10 d√≠gitos

                return `(${phone.slice(0, 3)}) ${phone.slice(
                    3,
                    6
                )}-${phone.slice(6)}`;
            };

            // Inicializar la aplicaci√≥n
            document.addEventListener("DOMContentLoaded", function () {
                allData = ALL_DATA;

                initializeData();
                renderExecutiveSummary();
                renderSubjects();
                renderMessages();
                renderCalls();
                renderAttachments();
                renderContacts();

                setupSearchListeners();

                document.getElementById("investigatedNumbers").textContent =
                    allData.map((p) => FormatPhone(p.phone)).join(", ");
            });

            // Inicializar datos filtrados
            function initializeData() {
                filteredData.subjects = [...allData];
                filteredData.messages = [];
                filteredData.calls = [];
                filteredData.attachments = [];
                filteredData.contacts = [];

                allData.forEach((person) => {
                    // Add contacts
                    if (person.contacts) {
                        person.contacts.forEach((contact) => {
                            filteredData.contacts.push({
                                ...contact,
                                personName: person.name,
                                personPhone: person.phone,
                            });
                        });
                    }

                    filteredData.contacts.push({
                        personName: person.name,
                        personPhone: person.phone,
                        contact_phone_number: person.phone,
                        firstname: person.name,
                        lastname: "",
                    });
                    if (person.messages) {
                        person.messages.forEach((msg) => {
                            filteredData.messages.push({
                                ...msg,
                                personName: person.name,
                                personPhone: person.phone,
                            });
                        });
                    }

                    if (person.calls) {
                        person.calls.forEach((call) => {
                            filteredData.calls.push({
                                ...call,
                                personName: person.name,
                                personPhone: person.phone,
                            });
                        });
                    }

                    if (person.messages) {
                        person.messages.forEach((msg) => {
                            // Procesar attachments regulares
                            if (msg.attachments) {
                                try {
                                    const attachments = JSON.parse(
                                        msg.attachments
                                    );
                                    attachments.forEach((url) => {
                                        filteredData.attachments.push({
                                            url: url,
                                            messageId: msg.id,
                                            sender: msg.sender,
                                            timestamp: msg.timestamp,
                                            personName: person.name,
                                            type: "attachment",
                                        });
                                    });
                                } catch (e) {
                                    console.error(
                                        "Error parsing attachments:",
                                        e
                                    );
                                }
                            }

                            // Procesar mensajes de audio
                            if (
                                msg.content &&
                                msg.content.includes("<!AUDIO-MESSAGE-IMAGE=")
                            ) {
                                const audioMatch = msg.content.match(
                                    /<!AUDIO-MESSAGE-IMAGE="([^"]+)"-AUDIO="([^"]+)"-DURATION="([^"]+)"!>/
                                );
                                if (audioMatch) {
                                    const imageUrl = audioMatch[1];
                                    const audioUrl = audioMatch[2];
                                    const duration = audioMatch[3];

                                    // Agregar la imagen del audio
                                    filteredData.attachments.push({
                                        url: imageUrl,
                                        messageId: msg.id,
                                        sender: msg.sender,
                                        timestamp: msg.timestamp,
                                        personName: person.name,
                                        type: "audio-image",
                                        duration: duration,
                                        audioUrl: audioUrl,
                                    });

                                    // Agregar el archivo de audio
                                    filteredData.attachments.push({
                                        url: audioUrl,
                                        messageId: msg.id,
                                        sender: msg.sender,
                                        timestamp: msg.timestamp,
                                        personName: person.name,
                                        type: "audio",
                                        duration: duration,
                                        imageUrl: imageUrl,
                                    });
                                }
                            }
                        });
                    }
                });
            } // Renderizar resumen ejecutivo
            function renderExecutiveSummary() {
                const totalMessages = filteredData.messages.length;
                const totalCalls = filteredData.calls.length;
                const totalAttachments = filteredData.attachments.length;
                const totalSubjects = filteredData.subjects.length;
                const totalContacts = filteredData.contacts.length;

                const answeredCalls = filteredData.calls.filter(
                    (call) => call.answered === 1
                ).length;
                const missedCalls = filteredData.calls.filter(
                    (call) => call.answered === 0
                ).length;

                // Contar mensajes de audio
                const audioMessages = filteredData.messages.filter((msg) =>
                    (msg.content || "").includes("<!AUDIO-MESSAGE-IMAGE=")
                ).length;

                const audioFiles = filteredData.attachments.filter(
                    (att) => att.type === "audio"
                ).length;

                const dateRange = getDateRange();

                document.getElementById("executiveSummary").innerHTML = `
                <div class="stat-card">
                    <div class="stat-number">${totalSubjects}</div>
                    <div class="stat-label">Sujetos Investigados</div>
                </div>
                <div class="stat-card">
                    <div class="stat-number">${totalContacts}</div>
                    <div class="stat-label">Contactos Encontrados</div>
                </div>
                <div class="stat-card">
                    <div class="stat-number">${totalMessages}</div>
                    <div class="stat-label">Mensajes Interceptados</div>
                </div>
                <div class="stat-card">
                    <div class="stat-number">${audioMessages}</div>
                    <div class="stat-label">Mensajes de Audio</div>
                </div>
                <div class="stat-card">
                    <div class="stat-number">${totalCalls}</div>
                    <div class="stat-label">Llamadas Registradas</div>
                </div>
                <div class="stat-card">
                    <div class="stat-number">${answeredCalls}</div>
                    <div class="stat-label">Llamadas Contestadas</div>
                </div>
                <div class="stat-card">
                    <div class="stat-number">${missedCalls}</div>
                    <div class="stat-label">Llamadas Perdidas</div>
                </div>
                <div class="stat-card">
                    <div class="stat-number">${totalAttachments}</div>
                    <div class="stat-label">Archivos Adjuntos</div>
                </div>
                <div class="stat-card">
                    <div class="stat-number">${audioFiles}</div>
                    <div class="stat-label">Archivos de Audio</div>
                </div>
            `;
            }

            // Obtener rango de fechas
            function getDateRange() {
                const allTimestamps = [
                    ...filteredData.messages.map((m) => m.timestamp),
                    ...filteredData.calls.map((c) => c.timestamp),
                ];

                if (allTimestamps.length === 0) return "N/A";

                allTimestamps.sort();
                const firstDate = new Date(allTimestamps[0]).toLocaleDateString(
                    "es-ES"
                );
                const lastDate = new Date(
                    allTimestamps[allTimestamps.length - 1]
                ).toLocaleDateString("es-ES");

                return `${firstDate} - ${lastDate}`;
            }

            // Obtener nombre de contacto por n√∫mero de tel√©fono
            function getContactName(phoneNumber) {
                const contact = filteredData.contacts.find(
                    (c) => c.contact_phone_number === phoneNumber
                );
                if (contact) {
                    const fullName =
                        `${contact.firstname} ${contact.lastname}`.trim();
                    return fullName || "Sin nombre";
                }
                return "No agregado";
            }

            // Renderizar sujetos
            function renderSubjects() {
                const container = document.getElementById("subjectsContainer");

                if (filteredData.subjects.length === 0) {
                    container.innerHTML =
                        '<div class="no-results">No se encontraron sujetos</div>';
                    return;
                }

                container.innerHTML = filteredData.subjects
                    .map(
                        (person) => `
                <div class="person-card">
                    <div class="person-header">
                        <div class="person-info">
                            <strong>Nombre:</strong> ${person.name}
                        </div>
                        <div class="person-info">
                            <strong>Tel√©fono:</strong> ${FormatPhone(
                                person.phone
                            )}
                        </div>
                        <div class="person-info">
                            <strong>ID Ciudadano:</strong> ${person.citizenid}
                        </div>
                        <div class="person-info">
                            <strong>Mensajes:</strong> ${
                                person?.messages?.length ?? 0
                            }
                        </div>
                        <div class="person-info">
                            <strong>Llamadas:</strong> ${
                                person?.calls?.length ?? 0
                            }
                        </div>
                    </div>
                </div>
            `
                    )
                    .join("");
            } // Renderizar mensajes
            function renderMessages() {
                const container = document.getElementById("messagesContainer");

                if (filteredData.messages.length === 0) {
                    container.innerHTML =
                        '<div class="no-results">No se encontraron mensajes</div>';
                    return;
                }

                if (messageGroupingEnabled) {
                    renderGroupedMessages(container);
                } else {
                    renderFlatMessages(container);
                }
            }

            // Renderizar mensajes agrupados por canal
            function renderGroupedMessages(container) {
                // Agrupar mensajes por canal (combinaci√≥n de remitente y receptor)
                const groupedMessages = {};

                filteredData.messages.forEach((msg) => {
                    // Obtener todos los contactos √∫nicos del mensaje para crear el canal
                    const participants = [msg.sender, msg.personPhone].sort();
                    const channelKey = msg.channel_id;

                    if (!groupedMessages[channelKey]) {
                        groupedMessages[channelKey] = {
                            participants: participants,
                            personName: msg.personName,
                            messages: [],
                        };
                    }
                    groupedMessages[channelKey].messages.push(msg);
                });

                // Ordenar mensajes dentro de cada canal
                Object.values(groupedMessages).forEach((channel) => {
                    channel.messages.sort((a, b) => {
                        const dateA = new Date(a.timestamp);
                        const dateB = new Date(b.timestamp);
                        return messageSortOrder === "asc"
                            ? dateA - dateB
                            : dateB - dateA;
                    });
                });

                // Ordenar canales por el mensaje m√°s reciente de cada uno
                const sortedChannels = Object.entries(groupedMessages).sort(
                    (a, b) => {
                        const latestA = new Date(
                            Math.max(
                                ...a[1].messages.map(
                                    (m) => new Date(m.timestamp)
                                )
                            )
                        );
                        const latestB = new Date(
                            Math.max(
                                ...b[1].messages.map(
                                    (m) => new Date(m.timestamp)
                                )
                            )
                        );
                        return messageSortOrder === "asc"
                            ? latestA - latestB
                            : latestB - latestA;
                    }
                );

                let html = "";
                sortedChannels.forEach(([channelKey, channel]) => {
                    const contactName1 = getContactName(
                        channel.participants[0]
                    );
                    const contactName2 = getContactName(
                        channel.participants[1]
                    );

                    // Aseguramos estructuras para selecci√≥n
                    if (!channelSelectionMode[channelKey])
                        channelSelectionMode[channelKey] = false;
                    if (!channelSelectedMessages[channelKey])
                        channelSelectedMessages[channelKey] = new Set();

                    html += `
                        <details id="details-${channelKey}" ${
                        channelSelectionMode[channelKey]
                            ? 'class="selection-mode"'
                            : ""
                    } open style="margin-bottom: 30px; border: 1px solid #4a9eff; border-radius: 10px; overflow: hidden;">
                            <summary style="background: linear-gradient(90deg, #4a9eff 0%, #0066cc 100%); color: white; padding: 15px; font-weight: bold; display:flex; justify-content:space-between; align-items:center;">
                                <div>
                                    <h4 style="margin: 0; display: flex; align-items: center; gap: 10px;">
                                        üì± Canal: ${channelKey}
                                        <span style="font-size: 12px; opacity: 0.8;">[${
                                            channel.messages.length
                                        } mensajes]</span>
                                    </h4>
                                    <div style="font-size: 12px; opacity: 0.9; margin-top: 5px;">
                                        Dispositivo de: ${channel.personName}
                                    </div>
                                </div>
                                <div style="display:flex; gap:8px; align-items:center;">
                                    <button onclick="toggleChannelSelection('${channelKey}'); event.stopPropagation();" style="padding:6px 10px; background:#fff; color:#0066cc; border-radius:6px; border:none; cursor:pointer;">Seleccionar</button>
                                </div>
                            </summary>
                            <div id="visualize-controls-${channelKey}" style="padding:10px; display: ${
                        channelSelectionMode[channelKey] ? "block" : "none"
                    }; background: rgba(255,255,255,0.02);">
                                <button onclick="visualizeSelected('${channelKey}')" style="background:#4a9eff;color:#fff;padding:8px 12px;border:none;border-radius:6px;cursor:pointer;">Visualizar seleccionados</button>
                                <button onclick="toggleChannelSelection('${channelKey}')" style="margin-left:8px;padding:8px 12px;border:none;border-radius:6px;cursor:pointer;background:#ccc;color:#000;">Cancelar</button>
                            </div>
                            <div class="table-container">
                                <table id="messages-table-${channelKey}" class="data-table">
                                    <thead>
                                        <tr>
                                            <th class="select-col">Sel</th>
                                            <th>ID</th>
                                            <th>Remitente</th>
                                            <th>Contacto</th>
                                            <th>Contenido</th>
                                            <th>Adjuntos</th>
                                            <th>Fecha/Hora</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        ${generateMessagesRows(
                                            channel.messages
                                        )}
                                    </tbody>
                                </table>
                            </div>
                        </details>
                    `;
                });

                container.innerHTML = html;
            }

            // Renderizar mensajes sin agrupar
            function renderFlatMessages(container) {
                // Ordenar todos los mensajes
                const sortedMessages = [...filteredData.messages].sort(
                    (a, b) => {
                        const dateA = new Date(a.timestamp);
                        const dateB = new Date(b.timestamp);
                        return messageSortOrder === "asc"
                            ? dateA - dateB
                            : dateB - dateA;
                    }
                );

                const messagesHtml = generateMessagesRows(sortedMessages);

                container.innerHTML = `
                    <div class="table-container">
                        <table class="data-table">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Dispositivo de</th>
                                    <th>Remitente</th>
                                    <th>Contacto</th>
                                    <th>Contenido</th>
                                    <th>Adjuntos</th>
                                    <th>Fecha/Hora</th>
                                </tr>
                            </thead>
                            <tbody>
                                ${messagesHtml}
                            </tbody>
                        </table>
                    </div>
                `;
            }

            // Generar filas de mensajes (funci√≥n auxiliar)
            function generateMessagesRows(messages) {
                return messages
                    .map((msg) => {
                        let contentHtml = "";
                        const content = msg.content || "";

                        if (content.includes("<!AUDIO-MESSAGE-IMAGE=")) {
                            const audioMatch = content.match(
                                /<!AUDIO-MESSAGE-IMAGE="([^"]+)"-AUDIO="([^"]+)"-DURATION="([^"]+)"!>/
                            );
                            if (audioMatch) {
                                const imageUrl = audioMatch[1];
                                const audioUrl = audioMatch[2];
                                const duration = audioMatch[3];
                                contentHtml = `
                                <div style="border: 1px solid #4a9eff; border-radius: 8px; padding: 10px; background: rgba(74, 158, 255, 0.1);">
                                    <div style="display: flex; align-items: center; gap: 10px; margin-bottom: 8px;">
                                        <span style="color: #4a9eff; font-weight: bold;">üéµ MENSAJE DE AUDIO</span>
                                        <span style="color: #ffc107; font-size: 12px;">‚è±Ô∏è ${duration}s</span>
                                    </div>
                                    <div style="display: flex; gap: 10px; align-items: center;">
                                        <img src="${imageUrl}" style="width: 60px; height: 40px; border-radius: 4px; cursor: pointer;" onclick="previewMedia('${imageUrl}')" title="Ver imagen del audio">
                                        <audio controls style="flex: 1;">
                                            <source src="${audioUrl}" type="audio/ogg">
                                            Tu navegador no soporta audio.
                                        </audio>
                                    </div>
                                </div>
                            `;
                            }
                        } else if (content.includes("<!SENT-LOCATION-X=")) {
                            const locationMatch = content.match(
                                /<!SENT-LOCATION-X=([^Y]+)Y=([^!]+)!>/
                            );
                            if (locationMatch) {
                                contentHtml = `<a target="_blank" href="https://did.lioncommunity.es/map?x=${locationMatch[1]}&y=${locationMatch[2]}" class="location-marker">üìç Ubicaci√≥n: X=${locationMatch[1]}, Y=${locationMatch[2]}</a>`;
                            }
                        } else if (content.includes("<!SENT-PAYMENT")) {
                            const paymentMatch = content.match(
                                /<!SENT-PAYMENT-([^Y]+)!>/
                            );
                            if (paymentMatch) {
                                contentHtml = `<span class="payment">üíµ Cantidad: $${paymentMatch[1]}</span>`;
                            }
                        } else if (content === "<!CALL-NO-ANSWER!>") {
                            contentHtml =
                                '<span style="color: #f44336;">üìû Llamada sin respuesta</span>';
                        } else {
                            contentHtml = content || "Sin contenido";
                        }

                        let attachmentsHtml = "";
                        if (msg.attachments) {
                            try {
                                const attachments = JSON.parse(msg.attachments);
                                attachmentsHtml = attachments
                                    .map(
                                        (url) =>
                                            `<span class="attachment-link" onclick="previewMedia('${url}')">${getMediaIcon(
                                                url
                                            )} ${getFileName(url)}</span>`
                                    )
                                    .join("<br>");
                            } catch (e) {
                                attachmentsHtml = "Error al cargar adjuntos";
                            }
                        }

                        // Diferentes formatos dependiendo si es vista agrupada o no
                        if (messageGroupingEnabled) {
                            // A√±adimos una columna de selecci√≥n (checkbox) cuando estamos en vista agrupada
                            return `
                            <tr id="message-${msg.id}">
                                <td class="select-col"><input type="checkbox" class="message-checkbox" data-channel="${
                                    msg.channel_id
                                }" data-id="${
                                msg.id
                            }" onchange="onMessageCheckboxChange(event)" ${
                                channelSelectedMessages[msg.channel_id] &&
                                channelSelectedMessages[msg.channel_id].has(
                                    String(msg.id)
                                )
                                    ? "checked"
                                    : ""
                            }></td>
                                <td>${msg.id}</td>
                                <td>${FormatPhone(msg.sender)}</td>
                                <td>${getContactName(msg.sender)}</td>
                                <td class="message-content">${contentHtml}</td>
                                <td>${attachmentsHtml || "N/A"}</td>
                                <td>${formatTimestamp(msg.timestamp)}</td>
                            </tr>
                        `;
                        } else {
                            return `
                            <tr id="message-${msg.id}">
                                <td>${msg.id}</td>
                                <td>${msg.personName}</td>
                                <td>${FormatPhone(msg.sender)}</td>
                                <td>${getContactName(msg.sender)}</td>
                                <td class="message-content">${contentHtml}</td>
                                <td>${attachmentsHtml || "N/A"}</td>
                                <td>${formatTimestamp(msg.timestamp)}</td>
                            </tr>
                        `;
                        }
                    })
                    .join("");
            }

            // Renderizar llamadas
            function renderCalls() {
                const container = document.getElementById("callsContainer");

                if (filteredData.calls.length === 0) {
                    container.innerHTML =
                        '<div class="no-results">No se encontraron llamadas</div>';
                    return;
                }

                const callsHtml = filteredData.calls
                    .map(
                        (call) => `
                <tr>
                    <td>${call.id}</td>
                    <td>${call.personName}</td>
                    <td>${FormatPhone(call.caller)}</td>
                    <td>${FormatPhone(call.callee)}</td>
                    <td class="call-duration">${formatDuration(
                        call.duration
                    )}</td>
                    <td>
                        <span class="${
                            call.answered ? "call-answered" : "call-missed"
                        }">
                            ${call.answered ? "‚úÖ Contestada" : "‚ùå Perdida"}
                        </span>
                    </td>
                    <td>${call.hide_caller_id ? "S√≠" : "No"}</td>
                    <td>${formatTimestamp(call.timestamp)}</td>
                </tr>
            `
                    )
                    .join("");
                container.innerHTML = `
                <div class="table-container">
                    <table class="data-table">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Dispositivo de</th>
                                <th>Llamante</th>
                                <th>Receptor</th>
                                <th>Duraci√≥n</th>
                                <th>Estado</th>
                                <th>ID Oculto</th>
                                <th>Fecha/Hora</th>
                            </tr>
                        </thead>
                        <tbody>
                            ${callsHtml}
                        </tbody>
                    </table>
                </div>
            `;
            }

            // Renderizar archivos adjuntos
            function renderAttachments() {
                const container = document.getElementById(
                    "attachmentsContainer"
                );

                if (filteredData.attachments.length === 0) {
                    container.innerHTML =
                        '<div class="no-results">No se encontraron archivos adjuntos</div>';
                    return;
                }
                const attachmentsHtml = filteredData.attachments
                    .map((attachment) => {
                        let fileDisplay = "";
                        let fileType = "";

                        if (attachment.type === "audio") {
                            fileDisplay = `
                                    <div style="display: flex; align-items: center; gap: 8px;">
                                        <span style="color: #4a9eff;">üéµ</span>
                                        <span>${getFileName(
                                            attachment.url
                                        )}</span>
                                        <span style="color: #ffc107; font-size: 11px;">(${
                                            attachment.duration
                                        }s)</span>
                                        <audio controls style="width: 150px; height: 30px;">
                                            <source src="${
                                                attachment.url
                                            }" type="audio/ogg">
                                        </audio>
                                    </div>
                                `;
                            fileType = `Audio (${attachment.duration}s)`;
                        } else if (attachment.type === "audio-image") {
                            fileDisplay = `
                                    <div style="display: flex; align-items: center; gap: 8px;">
                                        <span style="color: #4a9eff;">üñºÔ∏èüéµ</span>
                                        <a href="#" class="attachment-link" onclick="previewMedia('${
                                            attachment.url
                                        }')">
                                            ${getFileName(attachment.url)}
                                        </a>
                                        <span style="color: #ffc107; font-size: 11px;">(Imagen de audio ${
                                            attachment.duration
                                        }s)</span>
                                    </div>
                                `;
                            fileType = `Imagen de Audio (${attachment.duration}s)`;
                        } else {
                            fileDisplay = `
                                    <a href="#" class="attachment-link" onclick="previewMedia('${
                                        attachment.url
                                    }')">
                                        ${getMediaIcon(
                                            attachment.url
                                        )} ${getFileName(attachment.url)}
                                    </a>
                                `;
                            fileType = getFileType(attachment.url);
                        }

                        return `
                                <tr>
                                    <td>${attachment.messageId}</td>
                                    <td>${attachment.personName}</td>
                                    <td>${FormatPhone(attachment.sender)}</td>
                                    <td>${fileDisplay}</td>
                                    <td>${fileType}</td>
                                    <td>${formatTimestamp(
                                        attachment.timestamp
                                    )}</td>
                                </tr>
                            `;
                    })
                    .join("");
                container.innerHTML = `
                <div class="table-container">
                    <table class="data-table">
                        <thead>
                            <tr>
                                <th>ID Mensaje</th>
                                <th>Sujeto</th>
                                <th>Remitente</th>
                                <th>Archivo</th>
                                <th>Tipo</th>
                                <th>Fecha/Hora</th>
                            </tr>
                        </thead>
                        <tbody>
                            ${attachmentsHtml}
                        </tbody>
                    </table>
                </div>
            `;
            }

            function collapseAllDetails() {
                document
                    .querySelectorAll("#messagesContainer details")
                    .forEach((d) => {
                        d.removeAttribute("open");
                    });
            }

            function expandAllDetails() {
                document
                    .querySelectorAll("#messagesContainer details")
                    .forEach((d) => {
                        d.setAttribute("open", "true");
                    });
            }

            // Manejo de selecci√≥n por canal
            function onMessageCheckboxChange(e) {
                const cb = e.target;
                const channel = cb.dataset.channel;
                const id = cb.dataset.id;
                if (!channelSelectedMessages[channel])
                    channelSelectedMessages[channel] = new Set();
                if (cb.checked) {
                    channelSelectedMessages[channel].add(id);
                } else {
                    channelSelectedMessages[channel].delete(id);
                }
                // Mostrar/ocultar bot√≥n de visualizar seg√∫n selecci√≥n
                updateChannelUI(channel);
            }

            function toggleChannelSelection(channelKey) {
                channelSelectionMode[channelKey] =
                    !channelSelectionMode[channelKey];
                const details = document.getElementById(
                    `details-${channelKey}`
                );
                const controls = document.getElementById(
                    `visualize-controls-${channelKey}`
                );
                if (channelSelectionMode[channelKey]) {
                    // activar modo selecci√≥n
                    if (details) details.classList.add("selection-mode");
                    if (controls) controls.style.display = "block";
                    // limpiar selecci√≥n previa
                    channelSelectedMessages[channelKey] = new Set();
                    // deschequear inputs
                    document
                        .querySelectorAll(
                            `#messages-table-${channelKey} .message-checkbox`
                        )
                        .forEach((cb) => {
                            cb.checked = false;
                        });
                } else {
                    // desactivar modo selecci√≥n
                    if (details) details.classList.remove("selection-mode");
                    if (controls) controls.style.display = "none";
                    channelSelectedMessages[channelKey] = new Set();
                }
            }

            function updateChannelUI(channelKey) {
                const controls = document.getElementById(
                    `visualize-controls-${channelKey}`
                );
                const set = channelSelectedMessages[channelKey];
                if (controls) {
                    if (set && set.size > 0) controls.style.display = "block";
                    else
                        controls.style.display = channelSelectionMode[
                            channelKey
                        ]
                            ? "block"
                            : "none";
                }
            }

            function visualizeSelected(channelKey) {
                const selected = channelSelectedMessages[channelKey]
                    ? Array.from(channelSelectedMessages[channelKey])
                    : [];
                if (!selected || selected.length === 0) {
                    alert("Seleccione al menos un mensaje para visualizar");
                    return;
                }

                // Obtener mensajes seleccionados desde filteredData
                const msgs = filteredData.messages
                    .filter(
                        (m) =>
                            m.channel_id == channelKey &&
                            selected.includes(String(m.id))
                    )
                    .sort(
                        (a, b) => new Date(a.timestamp) - new Date(b.timestamp)
                    );

                if (msgs.length === 0) {
                    alert(
                        "No se encontraron los mensajes seleccionados en los datos filtrados"
                    );
                    return;
                }

                const personPhone = msgs[0].personPhone; // propietario del dispositivo
                // Determinar el otro participante (el contacto con el que habla el propietario)
                const otherPhone =
                    msgs
                        .map((m) => m.sender)
                        .find((s) => String(s) !== String(personPhone)) ||
                    msgs[0].sender;

                // Buscar en los contactos filtrados por n√∫mero (varias claves posibles)
                const contact = filteredData.contacts.find(
                    (c) =>
                        c.contact_phone_number === String(otherPhone) ||
                        c.phone_number === String(otherPhone) ||
                        c.contact_phone_number === otherPhone ||
                        c.phone_number === otherPhone
                );
                const otherName = contact
                    ? `${contact.firstname || ""} ${
                          contact.lastname || ""
                      }`.trim() || FormatPhone(String(otherPhone))
                    : FormatPhone(String(otherPhone));
                const otherAvatar =
                    contact && contact.profile_image
                        ? contact.profile_image
                        : "";

                // Construir HTML de la nueva pesta√±a (simulaci√≥n iOS Messages)
                let html = `<!doctype html><html><head><meta charset="utf-8"><meta name="viewport" content="width=device-width,initial-scale=1"><title>Conversaci√≥n - ${otherName}</title>
                    <style>
                        body{font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, 'Helvetica Neue', Arial; background:#f0f0f2; margin:0; padding:20px;}
                        .phone{width:360px; margin:20px auto; background:#000; border-radius:30px; padding:12px; box-shadow:0 10px 30px rgba(0,0,0,0.4);}
                        .screen{background:#fff; border-radius:20px; padding:12px; height:640px; overflow:auto; scrollbar-width: none; -ms-overflow-style: none;}
                        .screen::-webkit-scrollbar {display: none;}
                        .header{display:flex; align-items:center; gap:10px; padding:8px 12px; border-bottom:1px solid #eee;}
                        .avatar{width:40px;height:40px;border-radius:50%;background:#ddd}
                        .name{font-weight:600}
                        .messages{padding:12px}
                        .bubble{max-width:70%; padding:10px 12px; border-radius:18px; margin:8px 0; display:inline-block; clear:both}
                        .me{background:#007aff;color:white; float:right; border-bottom-right-radius:4px}
                        .them{background:#e5e5ea;color:#000; float:left}
                        .time{display:block; font-size:11px; opacity:0.6; margin-top:4px}
                    </style>
                    </head><body>
                    <div class="phone">
                        <div class="screen">
                            <div class="header">${
                                otherAvatar
                                    ? `<img src="${otherAvatar}" class="avatar" style="object-fit:cover;">`
                                    : '<div class="avatar"></div>'
                            }<div><div class="name">${otherName}</div><div style="font-size:12px;opacity:0.6">Mensaje</div></div></div>
                            <div class="messages">
                `;

                msgs.forEach((m) => {
                    const isMe = String(m.sender) === String(personPhone);
                    let contentHtml = m.content || "";
                    if (contentHtml.includes("<!SENT-LOCATION-X=")) {
                        const match = contentHtml.match(
                            /<!SENT-LOCATION-X=([^Y]+)Y=([^!]+)!>/
                        );
                        if (match)
                            contentHtml = `<a target="_blank" style="text-decoration: none; color: inherit;" href="https://did.lioncommunity.es/map?x=${match[1]}&y=${match[2]}">üìç Ubicaci√≥n: X=${match[1]}, Y=${match[2]}</a>`;
                    } else if (contentHtml.includes("<!SENT-PAYMENT")) {
                        const pm = contentHtml.match(
                            /<!SENT-PAYMENT-([^Y]+)!>/
                        );
                        if (pm) contentHtml = `üíµ Cantidad: $${pm[1]}`;
                    } else if (contentHtml === "<!CALL-NO-ANSWER!>") {
                        contentHtml = "üìû Llamada sin respuesta";
                    }

                    // attachments
                    let attachmentsHtml = "";
                    if (m.attachments) {
                        try {
                            const atts = JSON.parse(m.attachments);
                            attachmentsHtml = atts
                                .map(
                                    (u) =>
                                        `<div style="margin-top:6px;"><img src="${u}" style="max-width:200px;border-radius:8px"></div>`
                                )
                                .join("");
                        } catch (e) {
                            attachmentsHtml = "";
                        }
                    }

                    html += `<div class="bubble ${
                        isMe ? "me" : "them"
                    }">${contentHtml}${attachmentsHtml}<span class="time">${new Date(
                        m.timestamp
                    ).toLocaleString()}</span></div>`;
                });

                html += `</div></div></div></body></html>`;

                const win = window.open("", "_blank");
                if (win) {
                    win.document.write(html);
                    win.document.close();
                } else {
                    alert(
                        "El navegador bloque√≥ la apertura de la nueva pesta√±a. Permita ventanas emergentes o pruebe manualmente."
                    );
                }

                // Opcional: salir del modo selecci√≥n
                toggleChannelSelection(channelKey);
            }

            // Renderizar contactos
            function renderContacts() {
                const container = document.getElementById("contactsContainer");

                if (filteredData.contacts.length === 0) {
                    container.innerHTML =
                        '<div class="no-results">No se encontraron contactos</div>';
                    return;
                }

                const contactsHtml = filteredData.contacts
                    .map((contact) => {
                        const fullName =
                            `${contact.firstname} ${contact.lastname}`.trim() ||
                            "Sin nombre";
                        const profileImage = contact.profile_image
                            ? `<img src="${contact.profile_image}" style="width: 40px; height: 40px; border-radius: 50%; object-fit: cover;" onerror="this.style.display='none'">`
                            : "üì±";
                        const isFavourite = contact.favourite ? "‚≠ê" : "";

                        return `
                <tr>
                    <td style="text-align: center;">${profileImage}</td>
                    <td>${fullName} ${isFavourite}</td>
                    <td>${FormatPhone(contact.contact_phone_number)}</td>
                    <td>${contact.email || "N/A"}</td>
                    <td>${contact.address || "N/A"}</td>
                    <td>${contact.personName}</td>
                    <td>${FormatPhone(contact.personPhone)}</td>
                </tr>
            `;
                    })
                    .join("");

                container.innerHTML = `
                <div class="table-container">
                    <table class="data-table">
                        <thead>
                            <tr>
                                <th>Foto</th>
                                <th>Nombre</th>
                                <th>Tel√©fono</th>
                                <th>Email</th>
                                <th>Direcci√≥n</th>
                                <th>Dispositivo de</th>
                                <th>N√∫mero del Dispositivo</th>
                            </tr>
                        </thead>
                        <tbody>
                            ${contactsHtml}
                        </tbody>
                    </table>
                </div>
            `;
            } // Configurar listeners de b√∫squeda
            function setupSearchListeners() {
                document
                    .getElementById("subjectsSearch")
                    ?.addEventListener("input", function (e) {
                        searchSubjects(e.target.value);
                    });

                document
                    .getElementById("messagesSearch")
                    ?.addEventListener("input", function (e) {
                        searchMessages(e.target.value);
                    });

                document
                    .getElementById("messageSortOrder")
                    ?.addEventListener("change", function (e) {
                        messageSortOrder = e.target.value;
                        renderMessages();
                    });

                document
                    .getElementById("callsSearch")
                    ?.addEventListener("input", function (e) {
                        searchCalls(e.target.value);
                    });

                document
                    .getElementById("attachmentsSearch")
                    ?.addEventListener("input", function (e) {
                        searchAttachments(e.target.value);
                    });

                document
                    .getElementById("contactsSearch")
                    ?.addEventListener("input", function (e) {
                        searchContacts(e.target.value);
                    });
            }

            // Funciones de b√∫squeda
            function searchSubjects(query) {
                if (!query.trim()) {
                    filteredData.subjects = [...allData];
                } else {
                    const searchTerm = query.toLowerCase();
                    filteredData.subjects = allData.filter(
                        (person) =>
                            person.name.toLowerCase().includes(searchTerm) ||
                            person.phone.includes(searchTerm) ||
                            person.citizenid.toLowerCase().includes(searchTerm)
                    );
                }
                renderSubjects();
            }
            function searchMessages(query) {
                if (!query.trim()) {
                    initializeData();
                } else {
                    const searchTerm = query.toLowerCase();
                    filteredData.messages = [];

                    // Buscar en todos los mensajes originales
                    allData.forEach((person) => {
                        if (person.messages) {
                            person.messages.forEach((msg) => {
                                const msgWithPerson = {
                                    ...msg,
                                    personName: person.name,
                                    personPhone: person.phone,
                                };

                                if (
                                    (msg.content || "")
                                        .toLowerCase()
                                        .includes(searchTerm) ||
                                    msg.sender.includes(searchTerm) ||
                                    msg.timestamp.includes(searchTerm) ||
                                    person.name
                                        .toLowerCase()
                                        .includes(searchTerm)
                                ) {
                                    filteredData.messages.push(msgWithPerson);
                                }
                            });
                        }
                    });
                }
                renderMessages();
            }

            function searchCalls(query) {
                if (!query.trim()) {
                    initializeData();
                    renderCalls();
                } else {
                    const searchTerm = query.toLowerCase();
                    filteredData.calls = filteredData.calls.filter(
                        (call) =>
                            call.caller.includes(searchTerm) ||
                            call.callee.includes(searchTerm) ||
                            call.timestamp.includes(searchTerm) ||
                            call.personName
                                .toLowerCase()
                                .includes(searchTerm) ||
                            call.duration.toString().includes(searchTerm)
                    );
                    renderCalls();
                }
            }
            function searchAttachments(query) {
                if (!query.trim()) {
                    initializeData();
                    renderAttachments();
                } else {
                    const searchTerm = query.toLowerCase();
                    filteredData.attachments = filteredData.attachments.filter(
                        (attachment) =>
                            attachment.url.toLowerCase().includes(searchTerm) ||
                            attachment.sender.includes(searchTerm) ||
                            attachment.personName
                                .toLowerCase()
                                .includes(searchTerm) ||
                            (attachment.type || "")
                                .toLowerCase()
                                .includes(searchTerm) ||
                            (attachment.duration || "")
                                .toString()
                                .includes(searchTerm) ||
                            getFileType(attachment.url)
                                .toLowerCase()
                                .includes(searchTerm)
                    );
                    renderAttachments();
                }
            }

            function searchContacts(query) {
                if (!query.trim()) {
                    initializeData();
                    renderContacts();
                } else {
                    const searchTerm = query.toLowerCase();
                    filteredData.contacts = filteredData.contacts.filter(
                        (contact) =>
                            (contact.firstname || "")
                                .toLowerCase()
                                .includes(searchTerm) ||
                            (contact.lastname || "")
                                .toLowerCase()
                                .includes(searchTerm) ||
                            contact.contact_phone_number.includes(searchTerm) ||
                            (contact.email || "")
                                .toLowerCase()
                                .includes(searchTerm) ||
                            contact.personName
                                .toLowerCase()
                                .includes(searchTerm) ||
                            contact.personPhone.includes(searchTerm)
                    );
                    renderContacts();
                }
            } // Funci√≥n para alternar secciones
            function toggleSection(header) {
                const section = header.parentElement;
                section.classList.toggle("collapsed");
            }

            // Funci√≥n para alternar agrupamiento de mensajes
            function toggleMessageGrouping() {
                messageGroupingEnabled = !messageGroupingEnabled;
                const button = document.getElementById("groupToggleBtn");
                if (messageGroupingEnabled) {
                    button.textContent = "üì± Agrupar por canal";
                    button.style.background = "#4a9eff";
                } else {
                    button.textContent = "üìã Vista plana";
                    button.style.background = "#28a745";
                }
                renderMessages();
            }

            // Funciones de utilidad
            function formatTimestamp(timestamp) {
                return new Date(timestamp).toLocaleString("es-ES");
            }

            function formatDuration(seconds) {
                const minutes = Math.floor(seconds / 60);
                const remainingSeconds = seconds % 60;
                return `${minutes}:${remainingSeconds
                    .toString()
                    .padStart(2, "0")}`;
            }

            function getMediaIcon(url) {
                const extension = url.split(".").pop().toLowerCase();
                switch (extension) {
                    case "jpg":
                    case "jpeg":
                    case "png":
                    case "gif":
                    case "webp":
                        return "üñºÔ∏è";
                    case "mp4":
                    case "avi":
                    case "mov":
                        return "üé•";
                    case "mp3":
                    case "wav":
                    case "ogg":
                        return "üéµ";
                    default:
                        return "üìÑ";
                }
            }

            function getFileName(url) {
                return url.split("/").pop().split("?")[0];
            }

            function getFileType(url) {
                const extension = url.split(".").pop().toLowerCase();
                switch (extension) {
                    case "jpg":
                    case "jpeg":
                    case "png":
                    case "gif":
                    case "webp":
                        return "Imagen";
                    case "mp4":
                    case "avi":
                    case "mov":
                        return "Video";
                    case "mp3":
                    case "wav":
                    case "ogg":
                        return "Audio";
                    default:
                        return "Archivo";
                }
            }

            // Previsualizaci√≥n de medios
            function previewMedia(url) {
                const modal = document.getElementById("previewModal");
                const modalContent = document.getElementById("modalContent");

                const extension = url.split(".").pop().toLowerCase();

                let content = "";
                if (["jpg", "jpeg", "png", "gif", "webp"].includes(extension)) {
                    content = `<img src="${url}" class="preview-image" alt="Imagen adjunta">`;
                } else if (["mp4", "avi", "mov"].includes(extension)) {
                    content = `<video controls class="preview-image"><source src="${url}" type="video/${extension}"></video>`;
                } else if (["mp3", "wav", "ogg"].includes(extension)) {
                    content = `<audio controls style="width: 100%;"><source src="${url}" type="audio/${extension}"></audio>`;
                } else {
                    content = `<p>No se puede previsualizar este tipo de archivo.</p><a href="${url}" target="_blank">Abrir en nueva pesta√±a</a>`;
                }

                modalContent.innerHTML = content;
                modal.style.display = "block";
            }

            function closeModal() {
                document.getElementById("previewModal").style.display = "none";
            }

            // Cerrar modal al hacer clic fuera de √©l
            window.onclick = function (event) {
                const modal = document.getElementById("previewModal");
                if (event.target === modal) {
                    modal.style.display = "none";
                }
            };
        </script>
    </body>
</html>
